#include <stdio.h>
#define max 5
void enqueue(int queue[],int *front , int *rear , int item){
    if((*rear == max && *front == 1) || (*front == *rear +1)){
        printf("OVERFLOW!! \n");
        return;
    } 
    if(*front == -1 && *rear == -1){
        *rear = *front = 0;
    }
    else if(*rear == max){
        *rear = 0;
    }
    else *rear +=1;
    queue[*rear]=item;
}
int dequeue(int queue[] , int *front , int *rear ){
    if(*front == -1 && *rear == -1){
        printf("UNDERFLOW!!\n");
        return -1 ;
    }
    int value = queue[*front];
    if (*front == *rear){
        *front = *rear = -1;
    }
    else if ( *front == max ){
        *front = 0;
    }
    else *front = *front + 1;
    return value;
}
void display(int a[],int front ,int rear){
    for(int i=front; i<=rear; i++){
        printf("%d ",a[i]);
    }
    printf("\n");
}
int main() {
    int queue [max];
    int front = -1 , rear = -1;
    enqueue(queue,&front,&rear,14);
    enqueue(queue,&front,&rear,12);
    enqueue(queue,&front,&rear,10);
    enqueue(queue,&front,&rear,5);
    display(queue , front ,rear);
    dequeue(queue,&front,&rear);
    dequeue(queue,&front,&rear);
    display(queue ,front, rear);
    enqueue(queue,&front,&rear,12);
    display(queue ,front, rear);
    enqueue(queue,&front,&rear,50);
    printf("\n%d front , %d rear\n",front , rear);
    display(queue , front ,rear);
    

    return 0;
}
